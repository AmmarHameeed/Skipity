{
  "manifest_version": 3,
  "name": "Skipity",
  "version": "1.0",
  "description": "Automatically skips video intros",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.netflix.com/*", "*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
} 